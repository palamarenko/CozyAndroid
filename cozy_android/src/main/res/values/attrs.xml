<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CozyImageView">
        <attr name="back_shape" format="enum">
            <enum name="None" value="0"/>
            <enum name="Circle" value="1"/>
            <enum name="Rectangle" value="2"/>
            <enum name="RoundRect" value="3"/>
        </attr>
        <attr name="corners_radius" format="dimension"/>
        <attr name="back_shape_color" format="color" />
    </declare-styleable>
</resources>
